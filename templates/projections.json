{
  "backend/app/admin/*.rb": { "type": "admin" },
  "backend/app/models/*.rb": { "type": "model" },
  "backend/app/services/*.rb": { "type": "service" },
  "backend/app/graphql/types/*_type.rb": { "type": "type" },
  "backend/app/graphql/mutations/*.rb": { "type": "mutation" },
  "backend/app/graphal/resolvers/*.rb": { "type": "resolver" },
  "backend/spec/factories/*.rb": { "type": "factory" },
  "*.ts": { "alternate": "{}.test.ts" },
  "*.tsx": { "alternate": "{}.test.tsx" },
  "*.test.ts": { "alternate": "{}.ts" },
  "*.test.tsx": { "alternate": "{}.tsx" },
  "*.js": { "alternate": "{}.test.js" }
  "*.test.js": { "alternate": "{}.js" }
  "app/components/*.rb": {
    "type": "component",
    "alternate": "app/components/{}.html.erb"
  },
  "app/components/*.html.erb": {
    "alternate": "app/components/{}.rb"
  },
  "config/locales/bg.yml": {
    "alternate": "config/locales/en.yml"
  },
  "config/locales/en.yml": {
    "alternate": "config/locales/bg.yml"
  },
  "config/locales/*.bg.yml": {
    "alternate": "config/locales/{}.en.yml"
  },
  "config/locales/*.en.yml": {
    "alternate": "config/locales/{}.bg.yml"
  }
}
