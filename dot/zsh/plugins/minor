function gci () {
  git commit -m "$*"
}

function kill_dev_processes() {
  kill -9 $(lsof -wni tcp:3000 -t) 2> /dev/null
  kill -9 $(lsof -wni tcp:3001 -t) 2> /dev/null
  kill -9 $(lsof -wni tcp:5000 -t) 2> /dev/null
  kill -9 $(lsof -wni tcp:5001 -t) 2> /dev/null
  kill -9 $(lsof -wni tcp:5051 -t) 2> /dev/null
  pkill -9 ruby
  pkill -9 puma
  pkill -9 spring
  pkill -9 node
  pkill -9 flow
}
