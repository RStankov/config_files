# bundler
alias bi="bundle install"
alias bu="bundle update"
alias bo="bundle open"
alias be="bundle exec"

# rails
alias r="rails"

# git
alias ga="git add -A"
alias gp="git pull --rebase"
alias grc="git add .; git rebase --continue"
alias gr="git rebase -i"
alias gr10="git rebase -i HEAD~10"
alias gs="git stash"
alias gc="git commit"
alias gco="git checkout"
alias gl="git lol"
alias gd="git diff"
alias gdc="git diff --cached"

# ls
alias l="ls -lahF"

# unix
alias ..="cd .."
alias ...='../..'

# redis
alias redis-start="launchctl stop homebrew.mxcl.redis"
alias redis-stop="launchctl start  homebrew.mxcl.redis"
