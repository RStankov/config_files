{
	"app": {
		"name": "Currency Converter",
		"id": "net.exit4web.currencyconverter",
		"version": "0.91",
		"cacheTTL": 3600
	},
	"defaults": {
		"from": "EUR",
		"to": "USD"
	},
	"commands": [
		{
			"id": "convert",
			"cmd": "Convert",
			"title": "Currency",
			"subtitle": "Try ‘12 €‘, ‘12 € to $‘ or ‘€‘ and convert your currency",
