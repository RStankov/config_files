  &lt;item uid="translation" arg="'$TEXT'"&gt;
	&lt;title&gt;'$TEXT'&lt;/title&gt;
    &lt;subtitle&gt;in English&lt;/subtitle&gt;
	&lt;icon&gt;icon.png&lt;/icon&gt;
  &lt;/item&gt;
