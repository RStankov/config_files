		'subtitle' =&gt; 'Select item to mark as done',
		'icon' =&gt; 'icon.png',
		'valid' =&gt; 'no',
		'autocomplete' =&gt; $name
	);
	array_push( $results, $item);
endforeach;

if ( count( $results ) &gt; 0 ):
	echo $utils-&gt;toxml( $results );
endif;

} else {
	
$todoresults = array();
$todos = exec('osascript GetItemsfromProject.scpt {query}');
$todo = explode(",", $todos);

foreach( $todo as $name ):
	$first = substr($name, 0, 1);
	if ($first === " ")
		$name = substr($name, 1);
	$item = array(
		'uid' =&gt; 'projecttodolistitem',
		'arg' =&gt; $name,
		'title' =&gt; $name,
		'subtitle' =&gt; 'Select item to mark as done',
		'icon' =&gt; 'icon.png',
		'vaild' =&gt; 'yes'
	);
	array_push( $todoresults, $item);
endforeach;

if ( count( $todoresults ) &gt; 0 ):
	echo $utils-&gt;toxml( $todoresults );
endif;

}</string>
