$utils = new Workflows();

$arg = "{query}";

if (empty($arg))
	$isEmpty = "Is is empty";

if ( empty($arg) ) {
	
$results = array();
$projects = exec('osascript GetProjects.scpt');
$project = explode(",", $projects);

foreach( $project as $name ):
	$first = substr($name, 0, 1);
	if ($first === " ")
		$name = substr($name, 1);
	$item = array(
		'uid' =&gt; 'projectlistitem',
